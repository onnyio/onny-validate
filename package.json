{
  "name": "onny-validate",
  "version": "0.0.4",
  "description": "common validators for onny products",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js --format ./node_modules/eslint-config-onny/eslintFormat",
    "lint-fix": "eslint . --ext .js --format ./node_modules/eslint-config-onny/eslintFormat --fix",
    "lint-fix_test": "eslint ./test --ext .js --format ./node_modules/eslint-config-onny/eslintFormat --fix",
    "test_local": "mocha",
    "test": "nyc mocha && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onnyio/onny-validate.git"
  },
  "keywords": [
    "onny",
    "validate",
    "validator"
  ],
  "author": "Isaac Marotte <isaac@onny.io>",
  "contributors": [
    "Isaac Marotte <isaac@onny.io> (http://onny.io)"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/onnyio/onny-validate/issues"
  },
  "homepage": "https://github.com/onnyio/onny-validate#readme",
  "dependencies": {
    "onny-utils": "^1.0.1",
    "validator": "^7.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.13.0",
    "eslint-config-onny": "^1.0.1-0",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^10.2.0"
  }
}
